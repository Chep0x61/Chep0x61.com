model ProfessionalPath {
  id              Int       @id @default(autoincrement())
  translations   ProfessionalPathTranslation[]
  creationDate DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}

model ProfessionalPathTranslation {
  id              Int       @id @default(autoincrement())
  professionalPathId     Int
  professionalPath        ProfessionalPath  @relation(fields: [professionalPathId], references: [id])
  translationCode String
  title String
}